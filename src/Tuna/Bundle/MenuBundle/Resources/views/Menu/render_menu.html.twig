{% set menuHtml %}
    {% for item in menu.children %}
        {% include 'TunaCMSMenuBundle:Menu:render_menu_item.html.twig' with { item: item } %}
    {% endfor %}
{% endset %}

{% if options.wrap %}
    <ul class="menu menu--{{ name|lower }}">{{ menuHtml }}</ul>
{% else %}
    {{ menuHtml }}
{% endif %}


