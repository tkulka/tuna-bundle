{% if metadata is defined %}
    {% embed '@TunaCMSAdmin/_partials/accordion.html.twig' with {
    label: 'Metadata'|trans,
    display_error: not metadata.vars.valid
    } %}
        {% block content %}
            {{ form_errors(metadata) }}
            {{ form_widget(metadata) }}
        {% endblock %}
    {% endembed %}
{% endif %}
